{
  "name": "advanced.js.koans",
  "version": "1.0.0",
  "description": "In this set of examples and tests we cover advanced JavaScript concepts such as closure, inheritance, variable hoisting, 'this' with context switch...",
  "main": "index.js",
  "directories": {
    "test": "tests"
  },
  "devDependencies": {
    "babel-core": "6.18.2",
    "babel-loader": "6.2.7",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "6.18.0",
    "babel-preset-stage-0": "6.16.0",
    "chai": "3.3.0",
    "esdoc": "0.4.8",
    "eslint": "3.9.1",
    "eslint-plugin-import": "2.0.1",
    "live-server": "0.9.2",
    "mocha": "3.1.2",
    "npm-run-all": "3.1.1",
    "wallaby-webpack": "0.0.9",
    "webpack": "1.13.3"
  },
  "scripts": {
    "prestest": "npm install",
    "test": "mocha --compilers js:babel-core/register -w",
    "docs": "esdoc -c esdoc.json && open ./esdoc/index.html",
    "live": "live-server",
    "build": "webpack -w",
    "start": "npm-run-all --parallel build live",
    "debug": "node-debug src/index.js",
    "prestart": "npm install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hackages/advanced.js.koans.git"
  },
  "keywords": [
    "javascript",
    "testing"
  ],
  "author": "007@hackages.io",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hackages/advanced.js.koans/issues"
  },
  "homepage": "https://github.com/hackages/advanced.js.koans#readme"
}
